{
  "name": "react_express_starter",
  "version": "1.0.0",
  "description": "> Create full stack apps with React and Express. Run your client and server with a single command",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "lint": "eslint ./**/*.js --fix",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edwincorea/react_express_starter.git"
  },
  "author": "Edwin Corea",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edwincorea/react_express_starter/issues"
  },
  "homepage": "https://github.com/edwincorea/react_express_starter#readme",
  "dependencies": {
    "concurrently": "^3.5.1",
    "express": "^4.16.2"
  },
  "devDependencies": {
    "nodemon": "^1.14.7"
  }
}
